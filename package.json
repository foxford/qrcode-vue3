{
  "name": "qrcode-vue3",
  "type": "module",
  "version": "1.7.1",
  "private": false,
  "packageManager": "pnpm@9.1.0",
  "description": "Add a style and an image to your QR code Vue3",
  "author": "Ludovit Scholtz <ludovit@scholtz.sk>, Diadal <info@diadal.com.ng>",
  "license": "MIT",
  "homepage": "https://github.com/scholtz/qrcode-vue3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scholtz/qrcode-vue3.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/qr-code-vue3.es.js",
      "require": "./dist/qr-code-vue3.umd.js"
    },
    "./styles.css": "./dist/qr-code-vue3.css"
  },
  "main": "./dist/qr-code-vue3.umd.js",
  "module": "./dist/qr-code-vue3.es.js",
  "typings": "./dist/index.d.ts",
  "styles": "./dist/qr-code-vue3.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:vite": "vite build",
    "build": "rm dist -rf && vue-tsc && build:vite",
    "lint:fix": "eslint . --fix",
    "types:check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "qrcode-generator": "^2.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "canvas": "^3.2.1",
    "eslint": "^9.39.2",
    "vite-plugin-dts": "^4.5.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.1"
  }
}
